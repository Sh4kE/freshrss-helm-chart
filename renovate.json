{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "regexManagers": [
    {
      "fileMatch": ["(^|/)Chart.yaml$"],
      "matchStrings": [
        "# renovate: datasource=(?<datasource>.*?) depName=(?<depName>.*?)( versioning=(?<versioning>.*?))?\\s*(appVersion:)\\s+(\"){0,}(?<currentValue>[a-z0-9.-]*)(\"){0,}"
      ]
    }
  ]
}
